      ]
    },
    {
      "url": "127.0.0.1:3000/?info=case_type-page",
      "actions": [
        "click element [data-ga-label=\"appeal\"]",
        "wait for element #main-content a[href^=\"/en\"] to be visible",
        "click element #main-content a[href^=\"/en\"]",
        "wait for element #steps-appeal-case-type-form-case-type-income-tax-field to be visible",
