{
  "defaults": {
    "timeout": 30000,
    "standard": "WCAG2AA",
    "viewport": {
      "width": 600,
      "height": 1000
    },
    "concurrency": 1,
    "hideElements": ".new_cookie, .cookie_return_path, .cookie_usage, .new_cookie, #new_cookie, #cookie_banner, #cookie_setting_cookie_setting, #cookie_setting_form_cookie_setting"
  },
  "urls": [
    {
      "url": "http://127.0.0.1:3000/en",
      "actions": [
        "click element [data-ga-label=\"appeal\"]"
        
      ]
    },
    {"url": "http://127.0.0.1:3000/en/appeal",
      "actions": [
        "click element #main-content a[href^=\"/en\"]"
      ]
    
    },
    {
      "url": "127.0.0.1:3000/?info=case_type-page",
      "actions": [
        "click element [data-ga-label=\"appeal\"]",
        "wait for element #main-content a[href^=\"/en\"] to be visible",
        "click element #main-content a[href^=\"/en\"]",
        "wait for element #steps-appeal-case-type-form-case-type-income-tax-field to be visible",
        "click element #steps-appeal-case-type-form-case-type-income-tax-field",
        "wait for element #main-content [type=\"submit\"] to be visible",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/save_and_return",
      "actions": [
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/select_language",
      "actions": [
        "click element #select-language-save-language-form-language-english-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/challenge/decision",
      "actions": [
        "click element #main-content #steps-challenge-decision-form-challenged-decision-yes-field",
        "click element #main-content [type=\"submit\"]" 
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/challenge/decision_status",
      "actions": [
        "click element #main-content #steps-challenge-decision-status-form-challenged-decision-status-received-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/appeal/dispute_type",
      "actions": [
        "click element #main-content #steps-appeal-dispute-type-form-dispute-type-penalty-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/appeal/penalty_amount",
      "actions": [
        "check field #main-content #steps-appeal-penalty-amount-form-penalty-level-penalty-level-1-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "http://127.0.0.1:3000/en/steps/lateness/in_time",
      "actions": [
        "check field #steps-lateness-in-time-form-in-time-yes-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/user_type",
      "actions": [
        "check field #steps-details-user-type-form-user-type-taxpayer-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/taxpayer_type",
      "actions": [
        "check field #steps-details-taxpayer-type-form-taxpayer-type-individual-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/taxpayer_details",
      "actions": [
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-individual-first-name-field to John",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-individual-last-name-field to Smith",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-contact-address-field to 1 Test Road",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-contact-city-field to Test",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-contact-postcode-field to TE1 1TE",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-contact-country-field to United Kingdom",
        "set field #steps-details-taxpayer-individual-details-form-taxpayer-contact-email-field to test@hmcts.net",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/send_taxpayer_copy",
      "actions": [
        "check field #steps-details-send-application-details-form-send-application-details-no-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/has_representative",
      "actions": [
        "check field #steps-details-has-representative-form-has-representative-no-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/grounds_for_appeal",
      "actions": [
        "set field #steps-details-grounds-for-appeal-form-grounds-for-appeal-field to something",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/eu_exit",
      "actions": [
        "check field #steps-details-eu-exit-form-eu-exit-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/outcome",
      "actions": [
        "set field #steps-details-outcome-form-outcome-field to something",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/need_support",
      "actions": [
        "check field #steps-details-need-support-form-need-support-no-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/letter_upload_type",
      "actions": [
        "check field #steps-details-letter-upload-type-form-letter-upload-type-multiple-field",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/letter_upload",
      "actions": [
        "check field #steps-details-letter-upload-form-having-problems-uploading-having-problems-uploading-field",
        "set field #steps-details-letter-upload-form-having-problems-uploading-explanation-field to something",
        "click element #main-content [type=\"submit\"]"
      ]
    },
    {
      "url": "127.0.0.1:3000/en/steps/details/documents_upload_problems",
      "actions": [
        "click element #main-content [type=\"submit\"]"
      ]
    }
  ]
}
